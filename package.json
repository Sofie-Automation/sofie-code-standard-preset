		"validate:dev-dependencies": "yarn audit --groups devDependencies",
		"license-validate": "./bin/checkLicenses.mjs"
	},
	"files": [
		"/CHANGELOG.md",
		"/README.md",
		"/ts",
		"/bin",
		"/eslint",
		"/.prettierrc.json"
	],
